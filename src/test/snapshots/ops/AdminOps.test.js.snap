    "4accb4d0-56ec-4a28-a769-5275dbac3147": {
      "_id": "4accb4d0-56ec-4a28-a769-5275dbac3147",
      "context": "AUTHENTICATION_TREE_DECISION_NODE",
      "createdBy": "null",
      "creationDate": 0,
