    "1e8175a2-6114-415f-9b72-9fe15bdf3661": {
      "_id": "1e8175a2-6114-415f-9b72-9fe15bdf3661",
      "context": "AUTHENTICATION_TREE_DECISION_NODE",
      "createdBy": "null",
      "creationDate": 0,
