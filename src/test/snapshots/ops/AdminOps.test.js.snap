        "          choices.push("Goto OIDC App");",
        "          if (""+existingSession.get("profileType") === "persistent") {",
        "              choices.push("Goto Profile Page");",
        "        }",
        "          choices.push("Logout");",
        "    } else {",
        "          message = "<h4>No Session!</h4><p>"",
        "          choices.push("Login");",
        "    }",
