        " * Author: volker.scheuber@forgerock.com",
        " */",
        "(function () {",
        "    outcome = "true";",
        "",
        "    var anchor = "anchor-".concat(generateNumericToken('xxx'));",
        "    var halign = "left";",
        "    var message = "<h4>Current State Values</h4>".concat(",
        "        "<p><b>Shared State</b>:<br/>").concat(",
        "        sharedState.toString()).concat("</p>").concat(",
