    "92edf2c7-0bab-412c-a0da-82ad4f04505b": {
      "_id": "92edf2c7-0bab-412c-a0da-82ad4f04505b",
      "context": "AUTHENTICATION_TREE_DECISION_NODE",
      "createdBy": "null",
      "creationDate": 0,
      "default": false,
      "description": "null",
      "evaluatorVersion": "1.0",
      "language": "JAVASCRIPT",
      "lastModifiedBy": "null",
