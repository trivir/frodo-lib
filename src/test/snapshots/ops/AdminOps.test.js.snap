    "5e68fee3-047d-4fff-8e99-89fb5908f068": {
      "_id": "5e68fee3-047d-4fff-8e99-89fb5908f068",
      "context": "AUTHENTICATION_TREE_DECISION_NODE",
      "createdBy": "null",
