        "(function () {",
        "      outcome = "true";",
        "      var theme = "";",
        "      if (requestParameters.get("themeId")) {",
        "          theme = requestParameters.get("themeId").get(0);",
        "    }",
        "",
        "    var fr = JavaImporter(",
        "        org.forgerock.openam.auth.node.api.Action,",
        "        org.forgerock.openam.authentication.callbacks.PollingWaitCallback",
