      "scripts": {
        "021e434f-89b6-45fb-9d67-5147bc1650c3": {
          "_id": "021e434f-89b6-45fb-9d67-5147bc1650c3",
          "context": "AUTHENTICATION_TREE_DECISION_NODE",
          "createdBy": "null",
